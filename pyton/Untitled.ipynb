   "source": [
    "print(\"\\n#17\") # 최희현씨 답안\n",
    "# 17. 최대낙차\n",
    "# 최대 낙차 : 7\n",
    "box=[7,4,2,0,0,6,0,7,0]\n",
    "drop2=[]\n",
    "for i in range(len(box)):\n",
    "    drop1=[]\n",
    "    for j in box[i+1:]:\n",
    "        if box[i]>j:\n",
    "            drop1.append(j)\n",
    "    drop2.append(len(drop1))\n",
    "print(\"최대 낙차 :\",max(drop2))"
